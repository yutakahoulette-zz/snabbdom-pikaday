{
  "name": "snabbdom-pikaday",
  "version": "0.0.1",
  "description": "Simple snabbdom wrapper for pikaday",
  "main": "build.js",
  "style": "index.css",
  "scripts": {
    "test": "browserify -t babelify -t cssify test/index.js | tape-run | tap-spec || true",
    "budo": "budo page.js --live -- -t babelify",
    "postcss": "postcss -c postcss.js -w -l",
    "babel": "babel index.js -o build.js",
    "build-docs": "./build-docs.sh"
  },
  "author": "Yutaka Houlette",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yutakahoulette/snabbdom-pikaday.git"
  },
  "license": "MIT",
  "dependencies": {
    "pikaday": "^1.5.1",
    "ramda": "0.23.0",
    "snabbdom": "0.6.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "6.18.0",
    "babelify": "7.3.0",
    "browserify": "13.3.0",
    "budo": "9.4.1",
    "cssify": "^1.0.3",
    "cssnano": "^3.10.0",
    "ff-core": "0.0.4",
    "flyd": "0.2.2",
    "normalize.css": "^5.0.0",
    "postcss": "5.2.9",
    "postcss-cli": "2.6.0",
    "postcss-import": "9.1.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "tape-run": "^2.1.5"
  },
  "keywords": [
    "snabbdom",
    "pikaday"
  ],
  "bugs": {
    "url": "https://github.com/yutakahoulette/snabbdom-pikaday/issues"
  },
  "homepage": "https://github.com/yutakahoulette/snabbdom-pikaday#readme"
}
